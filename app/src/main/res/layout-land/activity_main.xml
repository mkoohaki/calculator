        app:layout_constraintEnd_toStartOf="@id/x10"
        app:layout_constraintBottom_toTopOf="@id/ln"/>

    <Button
        android:id="@+id/xy"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/xy"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
