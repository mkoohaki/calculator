        app:layout_constraintBottom_toTopOf="@id/eight"/>

    <Button
        android:id="@+id/posneg"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/funcblackh"
        android:text="@string/posneg"
        android:textColor="@android:color/white"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/ac"
        app:layout_constraintBottom_toTopOf="@id/seven"/>

    <TextView
        android:id="@+id/screen"
        android:layout_width="@dimen/screenh_width"
        android:layout_height="@dimen/screenh_height"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/screen"
        android:gravity="start"
        android:paddingStart="@dimen/screen_padding"
        android:textColor="@android:color/black"
        android:textSize="@dimen/texth_size_screen"
        android:layout_marginTop="@dimen/margin_right"
        android:layout_marginStart="@dimen/margin_right"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/function"
        android:layout_width="@dimen/function_width"
        android:layout_height="@dimen/screenh_height"
        android:background="@drawable/function"
        android:textAlignment="center"
        android:textSize="@dimen/texth_size_screen"
        android:layout_marginTop="@dimen/margin_right"
        android:layout_marginEnd="@dimen/margin_right"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <Button
        android:id="@+id/rand"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/rand"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/zero"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <Button
        android:id="@+id/pi"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        android:background="@drawable/functionh"
        android:text="@string/pi"
        android:textColor="@android:color/white"
        android:textSize="@dimen/func_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/rand" />

    <Button
        android:id="@+id/tanh"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        android:background="@drawable/functionh"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/tanh"
        android:textColor="@android:color/white"
        android:textSize="@dimen/func_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/pi" />

    <Button
        android:id="@+id/cosh"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        android:background="@drawable/functionh"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/cosh"
        android:textColor="@android:color/white"
        android:textSize="@dimen/func_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tanh" />

    <Button
        android:id="@+id/sinh"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        android:background="@drawable/functionh"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/sinh"
        android:textColor="@android:color/white"
        android:textSize="@dimen/func_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cosh" />

    <Button
        android:id="@+id/deg"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        android:background="@drawable/functionh"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/deg"
        android:textColor="@android:color/white"
        android:textSize="@dimen/func_text_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/sinh" />

    <Button
        android:id="@+id/ee"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/ee"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/one"
        app:layout_constraintBottom_toTopOf="@id/rand"/>

    <Button
        android:id="@+id/e"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/e"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/ee"
        app:layout_constraintBottom_toTopOf="@id/pi"/>

    <Button
        android:id="@+id/tan"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/tan"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/e"
        app:layout_constraintBottom_toTopOf="@id/tanh"/>

    <Button
        android:id="@+id/cos"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/cos"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/tan"
        app:layout_constraintBottom_toTopOf="@id/cosh"/>

    <Button
        android:id="@+id/sin"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/sin"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/cos"
        app:layout_constraintBottom_toTopOf="@id/sinh"/>

    <Button
        android:id="@+id/x"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/x"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
