        android:layout_width="@dimen/zeroh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/zeroh"
        android:paddingStart="@dimen/zero_number_padding"
        android:text="@string/zero"
        android:textAlignment="viewStart"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/point"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <Button
        android:id="@+id/point"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/point"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/equal"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <Button
        android:id="@+id/equal"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/funch"
        android:text="@string/equal"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/marginh_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/plus"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/funch"
        android:text="@string/plus"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/marginh_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintBottom_toTopOf="@+id/equal"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/minus"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/funch"
        android:text="@string/minus"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/marginh_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintBottom_toTopOf="@+id/plus"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/multiply"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/funch"
        android:text="@string/multiply"
        android:textSize="@dimen/multiply_sizeh"
        android:layout_marginEnd="@dimen/marginh_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintBottom_toTopOf="@+id/minus"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/div"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/funch"
        android:text="@string/div"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/marginh_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintBottom_toTopOf="@+id/multiply"
        app:layout_constraintEnd_toEndOf="parent"  />

    <Button
        android:id="@+id/one"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/one"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/two"
        app:layout_constraintBottom_toTopOf="@id/zero"/>

    <Button
        android:id="@+id/two"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/two"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/three"
        app:layout_constraintBottom_toTopOf="@id/zero"/>

    <Button
        android:id="@+id/three"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/three"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/plus"
        app:layout_constraintBottom_toTopOf="@id/point"/>

    <Button
        android:id="@+id/four"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/four"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/five"
        app:layout_constraintBottom_toTopOf="@id/one"/>

    <Button
        android:id="@+id/five"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/five"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/six"
        app:layout_constraintBottom_toTopOf="@id/two"/>

    <Button
        android:id="@+id/six"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/six"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/minus"
        app:layout_constraintBottom_toTopOf="@id/three"/>

    <Button
        android:id="@+id/seven"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/seven"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/eight"
        app:layout_constraintBottom_toTopOf="@id/four"/>

    <Button
        android:id="@+id/eight"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/eight"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/nine"
        app:layout_constraintBottom_toTopOf="@id/five"/>

    <Button
        android:id="@+id/nine"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/numberh"
        android:text="@string/nine"
        android:textAlignment="center"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/multiply"
        app:layout_constraintBottom_toTopOf="@id/six"/>

    <Button
        android:id="@+id/percent"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/funcblackh"
        android:text="@string/percant"
        android:textColor="@android:color/white"
        android:textSize="@dimen/text_sizeh"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/div"
        app:layout_constraintBottom_toTopOf="@id/nine"/>

    <Button
        android:id="@+id/ac"
        android:layout_width="@dimen/numberh_width"
