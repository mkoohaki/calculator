        android:textColor="@android:color/white"
        android:text="@string/rad2"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/rad3"
        app:layout_constraintBottom_toTopOf="@id/cos"/>

    <Button
        android:id="@+id/x1"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/x1"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/rad2"
        app:layout_constraintBottom_toTopOf="@id/sin"
        tools:ignore="DuplicateIds" />

    <Button
        android:id="@+id/x10"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/x10"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
        android:layout_marginBottom="@dimen/margin_bottom"
        app:layout_constraintEnd_toStartOf="@id/seven"
        app:layout_constraintBottom_toTopOf="@id/log"/>

    <Button
        android:id="@+id/ex"
        android:layout_width="@dimen/numberh_width"
        android:layout_height="@dimen/numberh_width"
        android:background="@drawable/functionh"
        android:textColor="@android:color/white"
        android:text="@string/ex"
        android:textSize="@dimen/func_text_size"
        android:layout_marginEnd="@dimen/margin_right"
